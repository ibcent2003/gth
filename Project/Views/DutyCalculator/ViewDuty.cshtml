@model Project.Models.DutyCalculatorModel
@{
    ViewBag.Title = "Used Vehicle Duty Calculator Result";
    Layout = "~/Views/Shared/_DutyCalculatorLayout.cshtml";
}

<div class="widget">
    <div class="widget-title">
        <h3 class="title">Used Vehicle Duty Calculator Result</h3> 
    </div>
  
    <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable">

        <tbody>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label1" class="auto-style8">Vehicle Type</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_vehicle_type">@Model.ValuationTPD.VehicleType</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label2" class="auto-style8">Year Of Manufacture</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_mYear">@Model.ValuationTPD.ManufactureYear</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label13" class="auto-style8">Vehicle Make</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_vmake">@Model.ValuationTPD.Make</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label15" class="auto-style8">Model</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_vmodel">@Model.ValuationTPD.Model @if (Model.ValuationTPD.SpecialFeatures != null) { @Model.ValuationTPD.SpecialFeatures } </span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label3" class="auto-style8">Home Del. Value-HDV</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_hdv">@Model.ValuationTPD.Currency @Model.ValuationTPD.HDV</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label6" class="auto-style8">CC</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_vcc">@Model.ValuationTPD.EngineCapacity</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label4" class="auto-style8">Depreciation Allowed</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_dep">@Model.UsedVehicleDutyResultForm.depreciation</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label7" class="auto-style8">FOB (Cost)</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_fob">@Model.UsedVehicleDutyResultForm.FOB</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label5" class="auto-style8">Freight</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_freight">@Model.UsedVehicleDutyResultForm.freight</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label8" class="auto-style8">Insurance</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_insurance">@Model.UsedVehicleDutyResultForm.insurance</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label9" class="auto-style8">CIF</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_cif">@Model.UsedVehicleDutyResultForm.cif @Model.ValuationTPD.Currency</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label10" class="auto-style8">Exchange Rate</span>
                </td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_rate">@Model.UsedVehicleDutyResultForm.rate</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label11" class="auto-style8">CIF in GHCedis</span>
                </td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_Ghcedis">@Model.UsedVehicleDutyResultForm.ghcedis</span></td>
                <td class="auto-style9">
                    <span id="dnn_ctr12242_DutyCalculatorResult_Label12" class="auto-style8">HS Code</span>
                </td>
                <td>
                    <span id="dnn_ctr12242_DutyCalculatorResult_code">@Model.ValuationTPD.AssessedHSCode</span>
                </td>
            </tr>
            <tr class="warning">
                <td class="auto-style6" colspan="4">
                    <h5><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty15">DUTIES AND TAXES</span></h5>
                </td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty0" class="auto-style8">Import Duty</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty">@Model.UsedVehicleDutyResultForm.Importduty</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty1" class="auto-style8">Processing Fee</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_ProcessingFee">@Model.UsedVehicleDutyResultForm.ProcessingFee</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty2" class="auto-style8">VAT</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_vat">@Model.UsedVehicleDutyResultForm.vat</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty4" class="auto-style8">Shipper</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_inspectionfee">@Model.UsedVehicleDutyResultForm.shppers</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty3" class="auto-style8">NHIL</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_nhil">@Model.UsedVehicleDutyResultForm.nhil</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty5" class="auto-style8">Interest Charge</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_InterestCharge">@Model.UsedVehicleDutyResultForm.interestCharge</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty6" class="auto-style8">Ecowas</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_ecowas">@Model.UsedVehicleDutyResultForm.ecowas</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty7" class="auto-style8">Network Charge</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_NetworkCharge">@Model.UsedVehicleDutyResultForm.networkCharge</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty8" class="auto-style8">EXIM</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_edif">@Model.UsedVehicleDutyResultForm.edif</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty9" class="auto-style8">NetCharge VAT</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_NetchargeVat">@Model.UsedVehicleDutyResultForm.NetchargeVat</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty10" class="auto-style8">Exam Fee</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_examfee">@Model.UsedVehicleDutyResultForm.examfee</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty11" class="auto-style8">NetCharge NHIL</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_NetchargeNhil">@Model.UsedVehicleDutyResultForm.NetchargeNhil</span></td>
            </tr>
            <tr class="warning">
                <td class="auto-style7"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty13" class="auto-style8">Special Imp Levy</span></td>
                <td class="auto-style4"><span id="dnn_ctr12242_DutyCalculatorResult_special">@Model.UsedVehicleDutyResultForm.specialLevy</span></td>
                <td class="auto-style9"><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty12" class="auto-style8">OverAge Penalty</span></td>
                <td><span id="dnn_ctr12242_DutyCalculatorResult_OverAge">@Model.UsedVehicleDutyResultForm.Overage</span></td>
            </tr>


            <tr class="warning">
                <td class="auto-style3" colspan="3">
                    <h6><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty14">TOTAL ESTIMATED DUTY PAYABLE IN LOCAL CURRENCY BEFORE 30% DEDUCTION</span></h6>
                </td>
                <td>
                    <h6>
                        <span id="dnn_ctr12242_DutyCalculatorResult_localcurrency" style="font-weight: 400">@Model.UsedVehicleDutyResultForm.localCurrency</span>
                    </h6>
                </td>
            </tr>

            <tr class="warning">
                <td class="auto-style3" colspan="3">
                    <h6><span id="dnn_ctr12242_DutyCalculatorResult_ImportDuty14">TOTAL ESTIMATED DUTY PAYABLE IN LOCAL CURRENCY AFTER 30% DEDUCTION</span></h6>
                </td>
                <td>
                    <h6>
                        <span id="dnn_ctr12242_DutyCalculatorResult_localcurrency" style="font-weight: 400">@Model.UsedVehicleDutyResultForm.localCurrencyafterdedution</span>
                    </h6>
                </td>
            </tr>

           

        </tbody>

    </table>

